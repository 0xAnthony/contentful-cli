{
  "name": "contentful-cli",
  "version": "0.5.0",
  "description": "Contentful CLI tool",
  "main": "bin/contentful.js",
  "bin": {
    "contentful": "bin/contentful.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful-cli.git"
  },
  "author": "Contentful <opensource@contentful.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run vendor:version && babel lib -d dist",
    "build:standalone": "npm run build && pkg --targets node6-macos-x64,node6-linux-x64,node6-win-x64 --debug --out-dir build .",
    "clean": "rimraf dist",
    "dev": "nodemon --exec npm run build -w lib",
    "lint": "eslint bin lib test",
    "test": "npm run test:coverage",
    "test:coverage": "BABEL_ENV=test nyc npm run test:unit",
    "test:html-coverage": "BABEL_ENV=test nyc --reporter=html npm run test:unit",
    "test:unit": "BABEL_ENV=test ava --verbose",
    "test:unit:watch": "BABEL_ENV=test ava --verbose --watch",
    "test:simulate-ci": "trevor",
    "vendor:version": "echo \"module.exports = '`cat package.json|json version`'\" > version.js",
    "precommit": "npm run lint",
    "prepush": "npm run test:unit"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "4.1.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "babel-template": "^6.24.1",
    "babel-types": "^6.24.1",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.3",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "pkg": "^3.0.4",
    "rimraf": "^2.6.1",
    "sinon": "2.2.0",
    "stream-buffers": "^3.0.1",
    "strip-ansi": "^3.0.1",
    "trevor": "^2.3.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.23.0",
    "bfj": "^3.1.3",
    "blessed": "^0.1.81",
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "cli-table2": "^0.2.0",
    "command-exists": "^1.2.2",
    "contentful-export": "^4.5.0",
    "contentful-import": "^4.4.3",
    "contentful-management": "^3.5.0",
    "diff": "^3.2.0",
    "emojic": "^1.1.11",
    "fast-json-patch": "^1.1.9",
    "figlet": "^1.2.0",
    "inquirer": "^3.0.6",
    "inquirer-select-directory": "^1.1.2",
    "json": "^9.0.6",
    "listr": "^0.12.0",
    "lodash": "^4.17.4",
    "markdown-cli": "^1.5.1",
    "mkdirp": "^0.5.1",
    "mz": "^2.6.0",
    "opn": "5.0.0",
    "shell-escape": "^0.2.0",
    "slug": "^0.9.1",
    "tar": "^3.1.3",
    "tree-kill": "^1.1.0",
    "wrap-ansi": "^2.1.0",
    "yargs": "^7.1.0"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "bin",
    "dist",
    "docs",
    "version.js"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pkg": {
    "scripts": [
      "./node_modules/blessed/lib/widgets/*.js",
      "./dist/**/*.js"
    ],
    "assets": [
      "./node_modules/figlet/fonts/Standard.flf",
      "./node_modules/opn/xdg-open"
    ]
  }
}
